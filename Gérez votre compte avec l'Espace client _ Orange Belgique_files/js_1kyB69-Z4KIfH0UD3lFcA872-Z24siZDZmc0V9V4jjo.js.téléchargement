else if(direction==='horizontal'){setDirection.bgHorizontal($this,bgOffset,bgStart);}}
else if((type==='foreground')&&(scrolling<=documentHeight)){if(direction==='vertical'){setDirection.vertical($this,transform,oldTransform);}
